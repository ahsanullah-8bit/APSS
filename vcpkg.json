{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "ai-powered-surveillance-system",
    "maintainers": "Ahsan Ullah <CS120212021@kkkuk.edu.pk>, Muhammad Zohaib Shahid <CS120212048@kkkuk.edu.pk>",
    "description": "APSS is an AI Powered (Vehicle) Surveillance System (weird name) for our BS CS FYP",
    "supports": "windows",
    "dependencies": [
        {
            "name": "pkgconf",
            "version>=": "2.4.3#1"
        },
        {
            "name": "opencv4",
            "default-features": false,
            "features": ["fs", "gstreamer", "eigen", "highgui", "jpeg", "png"],
            "version>=": "4.10.0"
        },
        {
            "name": "ffmpeg",
            "default-features": false,
            "features": ["avcodec","avformat","swscale","avdevice","avfilter","swresample","openssl","zlib","x264","x265","freetype","drawtext"],
            "version>=": "7.1.1#1"
        },
        {
            "name": "tbb",
            "version>=": "2022.0.0"
        },
        {
            "name": "eigen3",
            "version>=": "3.4.0"
        },
        {
            "name": "yaml-cpp",
            "version>=": "0.8.0#2"
        },
        {
            "name": "reflectcpp",
            "default-features": false,
            "features": ["yaml"],
            "version>=": "0.17.0"
        },
        {
            "name": "gtest",
            "version>=": "1.16.0"
        }
    ]
}
